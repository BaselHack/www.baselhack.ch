{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "codebar-ag/laravel-default-nova": "^1.2.2",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^10.0",
        "laravel/nova": "^4.22",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "league/flysystem-aws-s3-v3": "^3.0",
        "maatwebsite/excel": "^3.1",
        "maatwebsite/laravel-nova-excel": "^1.2",
        "predis/predis": "^2.1",
        "sammyjo20/lasso": "^3.1",
        "spatie/laravel-activitylog": "^4.7",
        "spatie/laravel-enum": "^3.0",
        "spatie/laravel-flash": "^1.9",
        "spatie/laravel-ignition": "^2.0",
        "symfony/http-client": "^6.2",
        "symfony/postmark-mailer": "^6.2",
        "spatie/laravel-honeypot": "^4.3",
        "spatie/laravel-newsletter": "^5.1",
        "spatie/laravel-ray": "^1.32",
        "spatie/laravel-sitemap": "^6.2",
        "spatie/laravel-translatable": "^6.3",
        "spatie/nova-translatable": "^3.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8",
        "fakerphp/faker": "^1.21",
        "friendsofphp/php-cs-fixer": "^3.14",
        "laravel/dusk": "^7.7",
        "laravel/pint": "^1.6",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.5",
        "nunomaduro/collision": "^7.0",
        "pestphp/pest": "^2.x-dev",
        "pestphp/pest-plugin-laravel": "^2.x-dev",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan vendor:publish --tag=nova-assets --ansi --force"
        ]
    }
}
